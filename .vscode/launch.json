    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "build & run",
            "runtimeExecutable": "/usr/local/bin/npm",
            "runtimeArgs": [
                "run",
                "dev",
            ],
            "autoAttachChildProcesses": true,
            "restart": true,
            "sourceMaps": true,
            "stopOnEntry": false,
            "console": "integratedTerminal",
            "env": {
                "NESRO": "NESRO"
            }
        }
    ]
}
